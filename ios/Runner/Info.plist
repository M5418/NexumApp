<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Nexum</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>nexum_app</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<false/>
			<key>NSExceptionDomains</key>
			<dict>
				<key>127.0.0.1</key>
				<dict>
					<key>NSIncludesSubdomains</key>
					<true/>
					<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
					<true/>
				</dict>
				<key>localhost</key>
				<dict>
					<key>NSIncludesSubdomains</key>
					<true/>
					<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
					<true/>
				</dict>
			</dict>
		</dict>
		<key>NSCameraUsageDescription</key>
		<string>Nexum needs camera access for live streaming, video calls, and capturing photos.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>Nexum needs microphone access for live streaming, voice messages, and audio recording.</string>
		<key>NSPhotoLibraryAddUsageDescription</key>
		<string>Nexum needs access to save photos and videos to your library.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Nexum needs access to your photo library to share photos and videos.</string>
		<key>NSContactsUsageDescription</key>
		<string>Nexum needs access to your contacts to help you connect with friends.</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>Nexum needs your location to show nearby content and events.</string>
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>Nexum needs your location to show nearby content and events.</string>
		<key>NSBluetoothAlwaysUsageDescription</key>
		<string>Nexum needs Bluetooth access for audio devices like headphones and speakers.</string>
		<key>NSBluetoothPeripheralUsageDescription</key>
		<string>Nexum needs Bluetooth access for audio devices like headphones and speakers.</string>
		<key>NSAppleMusicUsageDescription</key>
		<string>Nexum needs access to your music library to add music to your content.</string>
		<key>NSFaceIDUsageDescription</key>
		<string>Nexum uses Face ID for secure authentication.</string>
		<key>NSUserTrackingUsageDescription</key>
		<string>Nexum uses this to provide personalized content and ads.</string>
		<key>UIBackgroundModes</key>
		<array>
			<string>audio</string>
			<string>voip</string>
			<string>remote-notification</string>
			<string>fetch</string>
			<string>bluetooth-central</string>
			<string>bluetooth-peripheral</string>
		</array>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
		</array>
		<key>UIStatusBarHidden</key>
		<false/>
	</dict>
</plist>
